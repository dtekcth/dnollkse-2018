{
    "env": {
        "browser": true,
        "es6": true,
        "jquery": true,
        "node": true,
        "meteor": true
    },
    "globals":{
        "_": true,
        "swal": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
      "babel",
      "react"
    ],
    "rules": {
      "strict": 0,
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
}
